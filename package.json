{
  "name": "spa-with-node",
  "private": true,
  "dependencies": {
    "concurrently": "^5.1.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev": "concurrently --kill-others 'yarn dev-client' 'yarn dev-server'",
    "dev-server": "yarn --cwd ./packages/client start",
    "dev-client": "yarn --cwd ./packages/server start:dev"
  },
  "workspaces": [
    "packages/*"
  ]
}
